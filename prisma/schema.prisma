// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

generator erd {
  provider = "prisma-erd-generator"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://admin:admin@postgres-container:5432/mydb?schema=public"
}

model Company {
  id           Int    @id @default(autoincrement())
  collaborator String
  job          String
  salary       String
}
